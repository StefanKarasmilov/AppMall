<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="horizontal"
    tools:context=".anatomy.AnatomyActivity">


    <com.zechassault.zonemap.view.NoteImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/imageMapView"
        android:visibility="invisible"
        android:src="@drawable/anatomical_chart_back" />

    <com.zechassault.zonemap.view.NoteImageView
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/imageMapViewFront"
        android:src="@drawable/anatomical_chart_front" />

</RelativeLayout>
